{
	"name": "UFRGS-Asm",
	"displayName": "UFRGS Assembly",
	"description": "Suporte a sintaxe de assembly dos processadores hipoteticos da UFRGS",
	"version": "1.0",
	"publisher": "nicolascoutochaves",
	"icon": "images/icon.png",
	"author": {
		"name": "Nicolas Chaves"
	},
	"categories": [
		"Programming Languages"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"galleryBanner": {
		"color": "#0084A9",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nicolascoutochaves/ufrgs-asm"
	},
	"contributes": {
		"languages": [
			{
				"id": "asm",
				"aliases": [
					"ced",
					"ahd",
					"rad"
				],
				"extensions": [
					".ced",
					".rad",
					".ahd"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "asm",
				"scopeName": "source.asm",
				"path": "./syntaxes/ufrgs-asm.json"
			}
		]
	}
}
